// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model User {
  id       Int     @id @default(autoincrement())
  email    String? @unique
  password String?
}

model Session {
  id  String  @id @default(uuid())
  userId     Int
}
